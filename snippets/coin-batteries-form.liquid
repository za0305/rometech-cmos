<form class="contact-warranty-form contact-form-with-media" action="/" method="post" accept-charset="utf-8" autocomplete="on" novalidate="true">
    <div class="posted-message">
        {% render 'icon-checkmark-circle' %}
        {{ section.settings.third_form_success }}
    </div>
    <input type="hidden" name="form" value="coin-batteries">
    <input type="hidden" name="location" value="rometechbatteries">
	<div class="form-all">
		<div class="form-line">
			<p class="form-subtitle">Where did you buy it?</p>
			<div class="form-multiple-column">
                <div class="form-column-3">
                    <input type="radio" class="form-radio form-radio-jointed" id="input_8_0" name="whereDidYouBuy" checked="" value="Amazon">
                    <label id="label_input_8_0" for="input_8_0">Amazon</label>
                </div>

                <div class="form-column-3">
                    <input type="radio" class="form-radio form-radio-jointed" id="input_8_1" name="whereDidYouBuy" value="Walmart">
                    <label id="label_input_8_1" for="input_8_1">Walmart</label>
                </div>

                <div class="form-column-3">
                    <input type="radio" class="form-radio form-radio-jointed" id="input_8_2" name="whereDidYouBuy" value="rometechproducts.com">
                    <label id="label_input_8_2" for="input_8_2">rometechproducts.com</label>
                </div>

                <div class="form-column-3">
                    <input type="radio" class="form-radio form-radio-jointed" id="input_8_3" name="whereDidYouBuy" value="eBay">
                    <label id="label_input_8_3" for="input_8_3">eBay</label>
                </div>

                <div class="form-column-3">
                    <input type="radio" class="form-radio form-radio-jointed" id="input_8_4" name="whereDidYouBuy" value="rometechcases.com">
                    <label id="label_input_8_4" for="input_8_4">rometechcases.com</label>
                </div>

                <div class="form-column-3">
                    <input type="radio" class="form-radio form-radio-jointed" id="input_8_5" name="whereDidYouBuy" value="rometechbatteries.com">
                    <label id="label_input_8_5" for="input_8_5">rometechbatteries.com</label>
                </div>

                <div class="form-column-3">
                    <input type="radio" class="form-radio form-radio-jointed" name="whereDidYouBuy" id="other_8" value="other">
                    <label id="label_other_8" for="other_8">Other</label>
                </div>
                
                <input type="text" class="form-radio-other-input form-textbox" name="otherPlace" size="15" id="input_7"  placeholder="Please type another option here">
			</div>
		</div>

		<h2 class="form-title">Order Details</h2>
		
		<div class="form-line">
			<p class="form-subtitle">Your Name<span class="form-required">*</span></p>
			<input type="text" name="yourName" class="form-textbox" placeholder="Enter Name Here" required="" value="">
		</div>

		<div class="form-line">
			<p class="form-subtitle">Email Address<span class="form-required">*</span></p>
			<input type="text" name="yourEmail" class="form-textbox" placeholder="youremail@mail.com" required="" value="">
            <p class="contact-warranty-error"></p>  
		</div>

		<div class="form-line">
			<p class="form-subtitle">Order ID<span class="form-required">*</span></p>
			<input type="text" id="input_19" name="orderId" class="form-textbox" placeholder="Enter Order ID Here" required="" value="">
			<label class="form-sub-label" for="input_19">Note: Amazon Order ID example is 123-1234567-1234567; eBay Order ID example is 12-12345-12345; Walmart Order ID example is 123456789012345; rometechcases.com Order ID example is SY1234; rometechproducts.com, rometechbatteries.com Order ID example is 12345</label>
		</div>

		<div class="form-line">
			<p class="form-subtitle">Purchase Date<span class="form-required">*</span></p>
			<input type="date" class="form-textbox form-date" name="purchaseDate" value="{{ 'now'| date: '%Y-%m-%d' }}" min="" max="">
		</div>

        <div class="form-line">
			<p class="form-subtitle">Automobile Make<span class="form-required">*</span></p>
			<input type="text" class="form-textbox" name="automobileMake" value="">
		</div>

        <div class="form-line">
			<p class="form-subtitle">Model<span class="form-required">*</span></p>
			<input type="text" class="form-textbox" name="model" value="">
		</div>

        <div class="form-line">
			<p class="form-subtitle">Year<span class="form-required">*</span></p>
            <input type="number" class="form-textbox" name="year" min="1900" max="2099" step="1" value="{{ 'now' |  date: '%Y' }}">
		</div>

        <div class="form-line">
			<p class="form-subtitle">Type of Batteries Needed<span class="form-required">*</span></p>
			<input type="text" class="form-textbox" name="typeBatteries" value="">
		</div>

        <div class="form-line">
			<p class="form-subtitle">Add a photo...</p>
			<div class="form-fileupload">
                <label class="fileupload-label">
                    <input class="fileupload-input" type="file" name="photo" accept="image/png, image/jpeg">
                </label>
			</div>
		</div>

		<div class="form-line">
			<button type="submit" class="form-submit-button button">Send</button>
		</div>
	</div>
</form>